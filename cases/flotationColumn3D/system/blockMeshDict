FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

scale   1;

W_inlet     0.03;
D_inlet     0.015;
W           0.2;
D           0.05;
H           0.5;

dx          0.003;

N1          #eval{ceil(($W - $W_inlet) / ($dx * 2.0))};
N2          #eval{ceil(($D - $D_inlet) / ($dx * 2.0))};
N3          #eval{ceil($W_inlet / $dx)};
N4          #eval{ceil($D_inlet / $dx)};

M           #eval{ceil($H / $dx)};

vertices
(


    (0                                0         0) //0
    (#eval{($W - $W_inlet) / 2.0}       0         0) //1
    (#eval{($W + $W_inlet) / 2.0}       0         0) //2
    (#eval{$W}                         0         0) //3

    (0                                0         #eval{($D - $D_inlet) / 2.0}) //4
    (#eval{($W - $W_inlet) / 2.0}       0         #eval{($D - $D_inlet) / 2.0}) //5
    (#eval{($W + $W_inlet) / 2.0}       0         #eval{($D - $D_inlet) / 2.0}) //6
    (#eval{$W}                         0         #eval{($D - $D_inlet) / 2.0}) //7

    (0                                0         #eval{($D + $D_inlet) / 2.0}) //8
    (#eval{($W - $W_inlet) / 2.0}       0         #eval{($D + $D_inlet) / 2.0}) //9
    (#eval{($W + $W_inlet) / 2.0}       0         #eval{($D + $D_inlet) / 2.0}) //10
    (#eval{$W}                         0         #eval{($D + $D_inlet) / 2.0}) //11

    (0                                0         #eval{$D}) //12
    (#eval{($W - $W_inlet) / 2.0}       0         #eval{$D}) //13
    (#eval{($W + $W_inlet) / 2.0}       0         #eval{$D}) //14
    (#eval{$W}                         0         #eval{$D}) //15


    (0                                #eval{$H}         0) //16
    (#eval{($W - $W_inlet) / 2.0}       #eval{$H}         0) //17
    (#eval{($W + $W_inlet) / 2.0}       #eval{$H}         0) //18
    (#eval{$W}                         #eval{$H}         0) //19

    (0                                #eval{$H}         #eval{($D - $D_inlet) / 2.0}) //20
    (#eval{($W - $W_inlet) / 2.0}       #eval{$H}         #eval{($D - $D_inlet) / 2.0}) //21
    (#eval{($W + $W_inlet) / 2.0}       #eval{$H}         #eval{($D - $D_inlet) / 2.0}) //22
    (#eval{$W}                         #eval{$H}         #eval{($D - $D_inlet) / 2.0}) //23

    (0                                #eval{$H}         #eval{($D + $D_inlet) / 2.0}) //24
    (#eval{($W - $W_inlet) / 2.0}       #eval{$H}         #eval{($D + $D_inlet) / 2.0}) //25
    (#eval{($W + $W_inlet) / 2.0}       #eval{$H}         #eval{($D + $D_inlet) / 2.0}) //26
    (#eval{$W}                         #eval{$H}         #eval{($D + $D_inlet) / 2.0}) //27

    (0                                #eval{$H}         #eval{$D}) //28
    (#eval{($W - $W_inlet) / 2.0}       #eval{$H}         #eval{$D}) //29
    (#eval{($W + $W_inlet) / 2.0}       #eval{$H}         #eval{$D}) //30
    (#eval{$W}                         #eval{$H}         #eval{$D}) //31

);


blocks
(
    hex (16 17 21 20 0 1 5 4) ($N1  $N2 $M) simpleGrading (1 1 1)
    hex (17 18 22 21 1 2 6 5) ($N3  $N2 $M) simpleGrading (1 1 1)
    hex (18 19 23 22 2 3 7 6) ($N1  $N2 $M) simpleGrading (1 1 1)

    hex (20 21 25 24 4 5 9 8) ($N1 $N4 $M) simpleGrading (1 1 1)
    hex (21 22 26 25 5 6 10 9) ($N3 $N4 $M) simpleGrading (1 1 1)
    hex (22 23 27 26 6 7 11 10) ($N1 $N4 $M) simpleGrading (1 1 1)

    hex (24 25 29 28 8 9 13 12) ($N1  $N2 $M) simpleGrading (1 1 1)
    hex (25 26 30 29 9 10 14 13 ) ($N3 $N2 $M) simpleGrading (1 1 1)
    hex (26 27 31 30 10 11 15 14 ) ($N1 $N2 $M) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    outlet
    {
        type patch;
        faces
        (
            (16 17 21 20)
            (17 18 22 21)
            (18 19 23 22)

            (20 21 25 24)
            (21 22 26 25)
            (22 23 27 26)

            (24 25 29 28)
            (25 26 30 29)
            (26 27 31 30)
        );
    }

    inlet 
    {
        type patch;
        faces
        (
            (5 6 10 9)
        );
    }
    
);

mergePatchPairs
(
);


