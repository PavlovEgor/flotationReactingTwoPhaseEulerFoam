FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.gas 1
    volScalarFieldValue alpha.liquid 0
    volScalarFieldValue pacticle.gas 0.01
    volScalarFieldValue particle.liquid 0.10

    // volScalarFieldValue alpha.gas 0.1
    // volScalarFieldValue alpha.liquid 0.9

    // volScalarFieldValue air.gas 0.5
    // volScalarFieldValue water.gas 0.48
    // volScalarFieldValue pacticle.gas 0.02

    // volScalarFieldValue air.liquid 0.08
    // volScalarFieldValue water.liquid 0.60
    // volScalarFieldValue particle.liquid 0.32
);

regions
(
    boxToCell
    {
        box (0 0 0) (0.15 0.501 0.1);
        fieldValues
        (
            volScalarFieldValue alpha.gas 0.01
            volScalarFieldValue alpha.liquid 0.99

            volScalarFieldValue air.gas 0.5
            volScalarFieldValue water.gas 0.48
            volScalarFieldValue pacticle.gas 0.02

            volScalarFieldValue air.liquid 0.08
            volScalarFieldValue water.liquid 0.60
            volScalarFieldValue particle.liquid 0.32
        );
    }
);


