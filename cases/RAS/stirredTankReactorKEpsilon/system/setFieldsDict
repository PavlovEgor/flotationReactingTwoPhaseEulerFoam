FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.gas 0.80
    volScalarFieldValue alpha.liqiud 0.20
    volScalarFieldValue particle.liquid 0.0
);

regions
(
    boxToCell
    {
        box (-0.21 -0.2 -0.21) (0.21 0.45 0.21);
        fieldValues
        (
                volScalarFieldValue alpha.gas 0.0
                volScalarFieldValue alpha.liquid 1.0
	        volScalarFieldValue particle.liquid 0.2
        );
    }
);


