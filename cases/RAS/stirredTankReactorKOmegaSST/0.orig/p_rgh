FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [1 -1 -2 0 0 0 0];

internalField       uniform 1e5;

boundaryField
{
    "(impeller|vessel)"
    {
        type               fixedFluxPressure;
        value              $internalField;
    }

    // outlet
    // {
    //     type            prghPressure;
    //     p               $internalField;
    //     value           $internalField;
    // }

    // outlet
    // {
    //     type            totalPressure;
    //     rho             thermo:rho.liquid;
    //     p0              uniform 1e5;
    //     value           uniform 1e5;

    //     // Optional entries
    //     U               U.liquid;
    //     phi             phi.liquid;
    // }

    // outlet
    // {
    //     type            zeroGradient;
    // }

    outlet
    {
        type            fixedFluxPressure;
        rho             rho;
        value           uniform 0;
    }
}

